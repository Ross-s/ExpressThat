{
  "name": "expressthat",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:staged": "prettier --write --cache --ignore-unknown",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:generate": "npx @better-auth/cli generate --output=./src/lib/drizzle/auth-schema.ts --y&& drizzle-kit generate --config=./src/lib/drizzle/drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config=./src/lib/drizzle/drizzle.config.ts --port=3001",
    "db:migrate": "drizzle-kit migrate --config=./src/lib/drizzle/drizzle.config.ts",
    "email:dev": "email dev --dir ./src/lib/email/templates/ --port 3006"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.828.0",
    "@better-auth/stripe": "^1.2.9",
    "@heroicons/react": "^2.2.0",
    "@libsql/client": "^0.15.9",
    "@marsidev/react-turnstile": "^1.1.0",
    "@react-email/components": "0.0.42",
    "babel-plugin-react-compiler": "^19.1.0-rc.2",
    "better-auth": "^1.2.10",
    "better-auth-harmony": "^1.2.5",
    "better-sqlite3": "^11.10.0",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.44.2",
    "next": "^15.4.0-canary.91",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-qr-code": "^2.0.16",
    "stripe": "^18.0.0"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.2.9",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/jest": "^29.5.14",
    "@types/node": "^24",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "daisyui": "^5.0.43",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "react-email": "4.0.16",
    "tailwindcss": "^4",
    "tsx": "^4.20.3",
    "typescript": "^5"
